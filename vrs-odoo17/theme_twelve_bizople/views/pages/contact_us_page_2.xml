<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="contact_us_two_template" model="ir.ui.view">
		<field name="name">Contact Us Two</field>
		<field name="type">qweb</field>
		<field name="key">website.contact_us_two_template</field>
		<field name="arch" type="xml">
			<t name="Contact Us Two" t-name="contact_us_two_template">
				<t t-call="website.layout">
					<div class="oe_structure"></div>
					<section class="s_contact_with_img">
						<div class="contact3 mini-spacer">
							<div class="row">
								<div class="container">
									<div class="row m-0">
										<div class="col-lg-6">
											<div class="card-shadow o_animate_preview o_animate o_we_selected_image o_anim_flip_in_y" style="animation-duration: 3s !important;">
												<img src="/theme_twelve_bizople/static/src/img/contact_img1.jpg"
													class="img-responsive" />
											</div>
										</div>
										<div class="col-lg-6">
											<div class="contact-box m-l-30">
												<h1 class="title font-light m-t-10">Quick Contact</h1>
												<section class="s_website_form pt16 pb16 o_colored_level" data-vcss="001" data-snippet="s_website_form" data-name="Form" style="background-image: none;">
													<div class="container">
													
														<form action="/website/form/" method="post" enctype="multipart/form-data" class="o_mark_required contact-form o_animate_preview o_animate o_anim_fade_in_right" data-mark="*" data-success-mode="redirect" data-success-page="/contactus-thank-you" data-model_name="crm.lead">
															<div class="s_website_form_rows row s_col_no_bgcolor">
																<div class="form-group  s_website_form_field col-lg-6  s_website_form_required" data-type="char" data-name="Field">
																	<label class="s_website_form_label" style="width: 200px" for="contactus2_form_name">
																		<span class="s_website_form_label_content">Your Name</span>
																		<span class="s_website_form_mark"> *</span>
																	</label>
																	
																		<input type="text" placeholder="Enter Your Name*" class="form-control s_website_form_input" name="contact_name" required="1" id="contactus2_form_name"/>
																	
																</div>

																<div class="form-group  s_website_form_field col-lg-6  s_website_form_required  " data-type="email" data-name="Field">
																	<label class="s_website_form_label " style="width: 200px" for="contactus2_form_email">
																		<span class="s_website_form_label_content">Your Email</span>
																		<span class="s_website_form_mark"> *</span>
																	</label>
																	
																		<input type="email" placeholder="Enter Your E-mail*" class="form-control s_website_form_input" name="email_from" required="1" id="contactus2_form_email"/>
																	
																</div>

																<div class="form-group  s_website_form_field col-12" data-type="tel" data-name="Field">
																	<label class="s_website_form_label " style="width: 200px" for="contactus2_form_phone">
																		<span class="s_website_form_label_content">Phone Number</span>
																	</label>
																	
																		<input type="tel" placeholder="Enter Your Phone Number" class="form-control s_website_form_input" name="phone" id="contactus2_form_phone"/>
																	
																</div>
																
																
																<div class="form-group  s_website_form_field col-12  s_website_form_model_required  " data-type="char" data-name="Field">
																	
																	<label class="s_website_form_label " style="width: 200px" for="contactus2_form_subject">
																		<span class="s_website_form_label_content">Subject</span>
																		<span class="s_website_form_mark"> *</span>
																	</label>
																	
																		<input type="text" placeholder="Enter Your Subject*" class="form-control s_website_form_input" name="name" required="true" id="contactus2_form_subject"/>
																</div>
																<div class="form-group  s_website_form_field col-12  s_website_form_required  " data-type="text" data-name="Field">
																	<label class="s_website_form_label " style="width: 200px" for="contactus2_form_message">
																		<span class="s_website_form_label_content">Message</span>
																		<span class="s_website_form_mark"> *</span>
																	</label>
																	
																		<textarea class="form-control s_website_form_input" name="description" placeholder="Enter Your Question*" required="1" id="contactus2_form_message" rows="3"/>
																</div>
																<div class="form-group  col-12 s_website_form_submit" data-name="Submit Button">
																	<a href="#" role="button" class="btn btn-danger-gradient  s_website_form_send o_default_snippet_text"><span class="icofont icofont-paper-plane mr-2" style="font-size: 17px; vertical-align: middle;"/>SUBMIT MESSAGE</a>
																	<span id="s_website_form_result"/>
																</div>
															</div>
														</form>
													</div>
												</section>
											</div>
										</div>
										<div class="col-lg-12">
											<div class="card m-t-40">
												<div class="row">
													<div class="col-lg-4 col-md-4">
														<div class=" d-flex no-block c-detail p-3">
															<div class="m-r-20 align-self-center">
																<img src="/theme_twelve_bizople/static/src/img/icon1.png" />
															</div>
															<div class="">
																<h6 class="font-medium">Address</h6>
																<p class="">
																	A/405, Business Campus
																	<br />
																	Ring Road
																</p>
															</div>
														</div>
													</div>
													<div class="col-lg-4 col-md-4">
														<div class=" d-flex no-block c-detail p-3">
															<div class="m-r-20 align-self-center">
																<img src="/theme_twelve_bizople/static/src/img/icon2.png" />
															</div>
															<div class="">
																<h6 class="font-medium">Phone</h6>
																<p class="">
																	251 546 9442
																	<br />
																	630 446
																	8851
																</p>
															</div>
														</div>
													</div>
													<div class="col-lg-4 col-md-4">
														<div class=" d-flex no-block c-detail p-3">
															<div class="m-r-20 align-self-center">
																<img src="/theme_twelve_bizople/static/src/img/icon3.png" />
															</div>
															<div class="">
																<h6 class="font-medium">Email</h6>
																<p class="">
																	info@bizople.com
																	<br />
																	www.bizople.com
																</p>
															</div>
														</div>
													</div>
												</div>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</section>

					<div class="oe_structure"></div>
				</t>
			</t>
		</field>
	</record>

	<record id="contact_us_two_template_menu" model="website.page">
		<field name="website_published">True</field>
		<field name="url">/contact-two</field>
		<field name="view_id" ref="contact_us_two_template" />
	</record>


</odoo>

