		<xpath expr="//div[@id='so_text_align']" position="before">
			<div id="btn_sizing" data-selector="a.btn, button.btn" data-exclude=".btn-link">
				<we-select string="Button Sizing">
					<we-button data-select-class="btn-sm">Small Button</we-button>
					<we-button data-select-class="btn-md">Medium Button</we-button>
					<we-button data-select-class="btn-lg">Large Button</we-button>
				</we-select>
			</div>
		</xpath>
