<?xml version="1.0" encoding="UTF-8"?>
<odoo>

	<template id="s_fitness_form_with_img" name="Form with image">
		<section class="s_fitness_form_with_img">
			<div class="container-fluid">
				<div class="row">
					<div class="col-lg-6 col-md-6">
						<div class="modal-bg p-40">
							<h2 class="font-light m-b-0">The goodness of</h2>
							<h1 class="font-light">Yoga &amp; Meditation</h1>
							<h6 class="subtitle m-t-20">
								Lorem ipsum dolor sit amet, constetr
								adipiscing
								elit.
							</h6>
							<section class="s_website_form pt16 pb16 o_colored_level" data-vcss="001" data-snippet="s_website_form" data-name="Form" style="background-image: none;">
								<div class="container">
								
									<form action="/website/form/" method="post" enctype="multipart/form-data" class="o_mark_required contact-form o_animate_preview o_animate o_anim_fade_in_right" data-mark="*" data-success-mode="redirect" data-success-page="/contactus-thank-you" data-model_name="crm.lead">
										<div class="s_website_form_rows row s_col_no_bgcolor">
											<div class="form-group  s_website_form_field col-12  s_website_form_required" data-type="char" data-name="Field">
												<label class="s_website_form_label  d-none" style="width: 200px" for="s_fitness_contactus6_form_name">
													<span class="s_website_form_label_content">Your Name</span>
													<span class="s_website_form_mark"> *</span>
												</label>
												
													<input type="text" placeholder="Enter Your Name*" class="form-control s_website_form_input" name="contact_name" required="1" id="s_fitness_contactus6_form_name"/>
												
											</div>

											<div class="form-group  s_website_form_field col-12  s_website_form_required  " data-type="email" data-name="Field">
												<label class="s_website_form_label  d-none" style="width: 200px" for="s_fitness_contactus6_form_email">
													<span class="s_website_form_label_content">Your Email</span>
													<span class="s_website_form_mark"> *</span>
												</label>
												
													<input type="email" placeholder="Enter Your E-mail*" class="form-control s_website_form_input" name="email_from" required="1" id="s_fitness_contactus6_form_email"/>
												
											</div>

											<div class="form-group  s_website_form_field col-12" data-type="tel" data-name="Field">
												<label class="s_website_form_label  d-none" style="width: 200px" for="s_fitness_contactus6_form_phone">
													<span class="s_website_form_label_content">Phone Number</span>
												</label>
												
													<input type="tel" placeholder="Enter Your Phone Number" class="form-control s_website_form_input" name="phone" id="s_fitness_contactus6_form_phone"/>
												
											</div>
											
											
											<div class="form-group  s_website_form_field col-12  s_website_form_model_required  " data-type="char" data-name="Field">
												
												<label class="s_website_form_label  d-none" style="width: 200px" for="s_fitness_contactus6_form_subject">
													<span class="s_website_form_label_content">Subject</span>
													<span class="s_website_form_mark"> *</span>
												</label>
												
													<input type="text" placeholder="Enter Your Subject*" class="form-control s_website_form_input" name="name" required="true" id="s_fitness_contactus6_form_subject"/>
											</div>
											<div class="form-group  s_website_form_field col-12  s_website_form_required  " data-type="text" data-name="Field">
												<label class="s_website_form_label  d-none" style="width: 200px" for="s_fitness_contactus6_form_message">
													<span class="s_website_form_label_content">Message</span>
													<span class="s_website_form_mark"> *</span>
												</label>
												
													<textarea class="form-control s_website_form_input" name="description" placeholder="Enter Your Question*" required="1" id="s_fitness_contactus6_form_message" rows="5"/>
											</div>
											<div class="form-group  col-6 s_website_form_submit" data-name="Submit Button">														
												<a type="submit"
													class="s_website_form_send btn btn-danger-gradient text-white btn-md rounded-circle o_default_snippet_text  arrow-btn">
														Register
														<i class="fa fa-1x fa-fw fa-long-arrow-right ml-1"></i>
												</a>
												<span id="s_website_form_result"/>
											</div>
											
										</div>
									</form>
								</div>
							</section>
						</div>
					</div>
					<div class="col-lg-6 col-md-6">
						<section class="modal-bg-img h-100" style="background-image: url('/theme_twelve_bizople/static/src/img/fitness_img1.jpg');">
						</section>
					</div>
				</div>
			</div>
		</section>
	</template>

</odoo>